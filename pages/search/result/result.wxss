/* 搜索框 */
.sousuo{
  width: 100%;
  height: 95rpx;
  display: flex;
  align-items: center;
  font-size: 24rpx;
  color: #000;
  position: fixed;
  background: #fff;
  border-bottom: 1rpx solid #F0F0F0;
  z-index: 110;
}
.sousuo input{
  width: 500rpx;
}
.sousuo_img{
  margin: 0 24rpx 0 31rpx;
  display: flex;
  align-items: center;
}
.text{
  color: #969696;
  font-size: 24rpx;
}
.cancel{
  position: absolute;
  right: 18rpx;
  font-size: 24rpx;
  color: #000;
}
.xiala{
  position: fixed;
  z-index: 100;
}
/* 购物车 */
.empty{
  width: 100%;
  height: 180rpx;
}
.item{
  overflow: hidden;
}
.information{
  display: inline-block;
  vertical-align: top;
  margin-right: 25rpx;
}
.informationImg{
  width:166rpx;
  height:166rpx;
  border-radius: 6rpx;
  margin-left: 33rpx;
}
.interval{
  margin-left: 33rpx;
}
.name{
  color: #282828;
  font-size: 28rpx;
}
.other{
  color: #646464;
  font-size: 24rpx;
}
.site{
  margin: 26rpx 0 24rpx 0;
}
.del {
    background-color: #b4282d;
    width: 136rpx;
    height: 230rpx;
    display: flex;
    flex-direction: column;
    align-items: center;
    justify-content: center;
    color: #fff;
    -webkit-transform: translateX(136rpx);
    transform: translateX(136rpx);
    -webkit-transition: all 0.4s;
    transition: all 0.4s;
  }
  .content {
    float: left;
    width: 100%;
    height: 230rpx;
    -webkit-transition: all 0.4s;
    transition: all 0.4s;
    -webkit-transform: translateX(136rpx);
    transform: translateX(136rpx);
    margin-left: -136rpx;
    display: flex;
    align-items: center;
  }
.touch-move-active .content,
.touch-move-active .del {
    -webkit-transform: translateX(0);
    transform: translateX(0);
}
.tarbar{
  width: 100%;
  height: 110rpx;
  position: fixed;
  bottom: 0;
  background: #F4F4F4;
  display: flex;
  align-items: center;
}
.allcheck{
  color: #BFBFBF;
  font-size: 24rpx;
  margin: 0 34rpx 0 13rpx;
}
.number{
  font-size: 28rpx;
  color: #282828;
}
.next{
  width: 180rpx;
  height: 80rpx;
  color: #FFFFFF;
  font-size: 24rpx;
  background: #2D70FC;
  border-radius: 40rpx;
  text-align: center;
  line-height: 80rpx;
  position: fixed;
  bottom:14rpx;
  right: 31rpx;
}
.kong{
  width: 100%;
  height: 120rpx;
}
/* 类型框 */
.fullbg3{
  position: fixed;
  top: 0;
  width: 100%;
  height: 100%;
  background:rgba(0,0,0,1);
