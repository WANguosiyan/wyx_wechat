/* 在线商城 */
.swiper-tab{
	width: 100%;
  height: 1rpx;
	text-align: center;
  position: absolute;
  bottom: 70rpx;
}
.swiper-tab-list{  
  font-size: 30rpx;
  display: inline-block;
	width: 30rpx;
  height: 6rpx;
  background: #F2F2F2;
  margin-right: 10rpx;
  border-radius: 3rpx;
}
.on{
  background: #F54440;
}
 
.swiper-box{ display: block; height: 317rpx; width: 100%; overflow: hidden; }
.item-box{
  width: 100%;
  height: 317rpx;
}
.online_store_Img{
  width:88rpx;
  height:120rpx;
  text-align:center;
  font-size: 0rpx;
  display: inline-block;
  margin-left: 50rpx;
  margin-bottom: 36rpx; 
}
.swiper_character{
  font-size: 20rpx;
  color: #727678;
}
/* 广告套餐 */
.SetMeal{
  padding: 0 32rpx;
  padding-bottom: 200rpx;
}
.SetMeal-nav{
  display: flex;
  font-size:32rpx;
  color:rgba(40,40,40,1);
  position: relative;
}
.SetMeal-address{
  display: flex;
  align-items: center;
  font-size:24rpx;
  color:rgba(69,74,83,1);
  position: absolute;
  right: 0;
}
.SetMeal-address-img{
  width: 32rpx;
  height: 32rpx;
  margin-left: 10rpx;
}
.SetMeal-item{
  position: relative;
  padding: 30rpx 0;
  border-bottom: 1rpx solid #EBEBEB;
}
.SetMeal-item-img{
  width:166rpx;
  height:166rpx;
  border-radius:6rpx;
  position: relative;
}
.SetMeal-item-info{
  display: inline-block;
  width: 500rpx;
  vertical-align: top;
  margin-left: 15rpx;
}
.SetMeal-sellOut{
  width: 102rpx;
  height: 102rpx;
  position: absolute;
  top: 50%;
  left: 50%;
  margin-left: -51rpx;
  margin-top: -51rpx;
  z-index: 10;
}
.SetMeal-item-title{
  font-size:28rpx;
  font-weight:bold;
}
.SetMeal-item-content{
  font-size:24rpx;
  color:rgba(100,100,100,1);
  margin-bottom: 20rpx;
  margin-top: 10rpx;
}
.SetMeal-money{
  color: #FF6633;
  font-size:24rpx;
}
.SetMeal-money-Actual{
  font-size:42rpx;
}
.SetMeal-money-Original{
  color: #959595;
  text-decoration:line-through;
}
.SetMeal-btn{
  width:110rpx;
  height:54rpx;
  background:rgba(45,112,252,1);
  box-shadow:0rpx 6rpx 6rpx 0rpx rgba(45,112,252,0.22);
  border-radius:27rpx;
  text-align: center;
  line-height: 54rpx;
  font-size:24rpx;
  color: #FFFFFF;
  position: absolute;
  bottom: 30rpx;
  right: 0;
}
.SetMeal-btnNot{
  width:110rpx;
  height:54rpx;
  background:rgba(180,180,180,1);
  box-shadow:0rpx 6rpx 6rpx 0rpx rgba(200,200,200,0.22);
  border-radius:27rpx;
  text-align: center;
  line-height: 54rpx;
  font-size:24rpx;
  color: #FFFFFF;
  position: absolute;
  bottom: 30rpx;
  right: 0;
}
/* 选择地区 */
.bg{
  width: 100%;
  height: 100%;
  position: fixed;
  z-index: 20;
  top: 0;
  left: 0;
  background: rgba(0,0,0,0.4);
}
.choose{
  width: 100%;
  height: 772rpx;
  background: #fff;
  position: fixed;
  left: 0;
  bottom: 108rpx;
  display: flex;
  font-size:28rpx;
  color:rgba(80,80,80,1);
  z-index: 30
}
.choose-le{
  width: 50%;
  height: 100%;
  overflow: scroll;

}
.choose-ri{
  width: 50%;
  height: 100%;
  background: #EAEAEA;
  border-left: 1px solid #D5D5D5;
  overflow: scroll;
}
.choose-le-item{
  height: 84rpx;
  border-bottom: 1px solid #D5D5D5;
  line-height: 84rpx;
  padding-left: 60rpx;
}
.choose-le-itembg{
  background: #F2F2F2;
}
.choose-ri-item{
  height: 84rpx;
  border-bottom: 1px solid #D5D5D5;
  line-height: 84rpx;
  padding-left: 60rpx;
  background: #F2F2F2;
}