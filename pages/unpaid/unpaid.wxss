.pay-time{
 width: 100%;
 height: 88rpx;
 background: #2D70FC;
 font-size: 20rpx;
 color: #fff;
 padding-top: 12rpx;
 position: fixed;
}
.pay-time view{
  margin-left: 32rpx;
}
.unpaid{
  margin-bottom: 14rpx;
  font-size: 28rpx;
  height: 28rpx;
}
.kong{
  width: 100%;
  height: 88rpx;
}
.order-info{
  font-size: 32rpx;
  color: #000000;
  margin: 26rpx 0 56rpx 32rpx;
}
.order-item{
  margin-left: 32rpx;
  margin-bottom: 32rpx;
}
.bg{
  width: 100%;
  height: 18rpx;
  background: #F4F4F4;
}
/* 广告播放相关信息 */
.play-time{
  height: 71rpx;
  line-height: 71rpx;
  font-size: 32rpx;
  color: #000000;
  border-bottom: 1rpx solid #F4F4F4;
}
.play-time view{
  margin-left: 32rpx;
}
.advertising-public{
  width: 702rpx;
  height: 99rpx;
  border-bottom: 1rpx solid #F4F4F4;
  margin-left: 32rpx;
  font-size: 28rpx;
  color: #272727;
  line-height: 99rpx;
}
.advertising-publicSD{
  width: 702rpx;
  border-bottom: 1rpx solid #F4F4F4;
  margin-left: 32rpx;
  font-size: 28rpx;
  color: #272727;
  display: inline-block;
  line-height: 99rpx;
}
.time{
  margin-bottom: 88rpx;
}
.advertising-public-L{
  float: left;
}
.advertising-public-R{
  margin-right: 16rpx;
  float: right;
}
.advertising-public-R1{
  width: 530rpx;
  display: inline-block;
}
.advertising-public-R1 view{
  display: inline-block;
  margin-left: 16rpx;
}
.time-frame{
  display: flex;
}
.time-frame view{
  margin-left: 38rpx;
}
.date{
  width: 702rpx;
  height: 97rpx;
  margin-left: 32rpx;
  font-size: 28rpx;
  color: #272727;
}
.date-L{
  float: left;
  margin: 36rpx 0 34rpx 0;
}
.date-R{
  float: right;
  margin-right: 16rpx;
  margin: 36rpx 0 34rpx 0;
}
.concrete-time{
  width: 702rpx;
  margin-left: 34rpx;
  margin-bottom: 16rpx;
  font-size: 24rpx;
  color: #282828;
  display: flex;
}
.years{
  width: 110rpx;
  margin-right: 30rpx;
}
.day{
   width: 600rpx;
  color: #515151;
}
.day text{
  display: inline-block;
  margin-right: 28rpx;
  line-height: 20rpx;
}
/* 订单信息 */
.pay-info{
  width: 100%;
}
.pay-box{
  width: 700rpx;
  height: 84rpx;
  border-bottom: 1rpx solid #E6E6E6;
  line-height: 84rpx;
  margin: auto;
}
.OrderInfo{
  font-size: 32rpx;
  color: #282828;
}
.pay-boxL{
  float: left;
  font-size: 28rpx;
  color: #282828;
}
.pay-boxR{
  float: right;
  font-size: 24rpx;
  color: #505050;
}
.total{
  width: 700rpx;
  height: 95rpx;
  line-height: 95rpx;
  margin: auto;
  font-size: 28rpx;
  color: #505050;
}
.totalR{
  float: right;
}
.fuhao{
  color: #FF4949
}
.money{
  color: #FF4949;
  font-size: 36rpx;
}
.flies{
  border: none;
}
.empty{
  width: 100%;
  height: 95rpx;
}
.pay-now{
  width: 100%;
  height: 95rpx;
  position: fixed;
  bottom: 0;
  background: #fff;
}
.pay-nowL{
  width:169rpx;
  height:64rpx;
  background:rgba(255,255,255,1);
  border:1rpx solid rgba(255, 71, 71, 1);
  border-radius:6rpx;
  font-size:24rpx;
  color:rgba(255,71,71,1);
  float: right;
  line-height: 65rpx;
  text-align: center;
  margin-right: 20rpx;
  margin-top: 16rpx;
}
.pay-nowR{
  width:169rpx;
  height:64rpx;
  background:rgba(255,255,255,1);
  border:1rpx solid rgba(255, 71, 71, 1);
  border-radius:6rpx;
  font-size:24rpx;
  color:rgba(255,71,71,1);
  float: right;
  line-height: 65rpx;
  text-align: center;
  margin-right: 24rpx;
  margin-top: 16rpx;
}
/* 取消框 */
.cancalBox{
  position: absolute;
  top: 50%;
  left: 50%;
  width: 400rpx;
  height: 250rpx;
  margin-left: -200rpx;
  margin-top: -100rpx;
  background: red;
  font-size: 32rpx;
}
/* 预览图片/视屏 */
.preview{
  width: 600rpx;
  height: 400rpx;
  margin: auto;
  margin-top: 60rpx;
  margin-bottom: 30rpx;
}
